{
  "extends": "airbnb-base",
  "globals": {
    "atom": "true"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-new": 0,
    "array-callback-return": 0,
    "no-console": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [
      2,
      {
        "ignore": [
          "atom"
        ]
      }
    ]
  }
}
